{
  "name": "chemfetch-supabase-live",
  "version": "1.0.0",
  "description": "Supabase database configuration and types for ChemFetch",
  "main": "supabase/database.types.ts",
  "scripts": {
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "fix-all": "npm run format && npm run lint:fix",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "supabase",
    "database",
    "typescript",
    "chemfetch"
  ],
  "author": "ChemFetch Team",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.sql": [
      "prettier --write"
    ]
  }
}
